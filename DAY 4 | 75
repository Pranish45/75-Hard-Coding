import numpy as np

x=np.array([[2,3],[5,6]])
y=np.array([[3,4],[7,8]])

print("Matrix of x\n",x)
print("Matrix of y\n",y)

print("Additon of matrices\n",np.add(x,y))

print("Subtraction of matrices\n",np.subtract(x,y))

print("Multiplication of matrices\n",np.dot(x,y))

print("Tranpose of matrix\n",np.transpose(x))

print("Tranpose of matrix\n",np.transpose(y))
